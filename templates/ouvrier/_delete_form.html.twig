<form method="post" action="{{ path('app_ouvrier_delete', {'id': ouvrier.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet ouvrier ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ouvrier.id) }}">
    <button type="submit" class="btn btn-danger">Supprimer</button>
</form>
